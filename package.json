{
  "author": "focus@kleegroup.com",
  "name": "focus-demo-app",
  "description": "Focus demonstration application",
  "version": "2.0.0",
  "homepage": "https://github.com/KleeGroup/focus-demo-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/KleeGroup/focus-demo-app.git"
  },
  "engines": {
    "node": "0.4.x",
    "npm": "2.14.x"
  },
  "scripts": {
    "bundle": "better-npm-run bundle",
    "build": "npm run bundle",
    "start": "npm run start:npm",
    "start:local": "better-npm-run webpack-dev-server-local-focus",
    "start:npm": "better-npm-run webpack-dev-server-npm-focus",
    "mocked-api": "node api/index.js"
  },
  "main": "app/index.js",
  "betterScripts": {
    "bundle": {
      "command": "webpack --progress",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "ENTRY_FILE_PATH": "./app/index",
        "BABELIFIED_PATH": "./app",
        "DEV": false,
        "NODE_ENV": "production",
        "LIBRARY_NAME": "FocusDemoApp",
        "MINIMIFY": false,
        "OUTPUT_DIR": "./public",
        "PACKAGE_JSON_PATH": "./"
      }
    },
    "webpack-dev-server-local-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "MINIMIFY": false,
        "API_PORT": 9999,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": true,
        "PACKAGE_JSON_PATH": "./"
      }
    },
    "webpack-dev-server-npm-focus": {
      "command": "node ./dev-server.js",
      "env": {
        "ANCHOR_CLASS": "demo-app",
        "BABELIFIED_PATH": "./app",
        "ENTRY_FILE_PATH": "./app/index",
        "PAGE_TITLE": "Focus demonstration application",
        "MINIMIFY": false,
        "API_PORT": 9999,
        "OUTPUT_DIR": "./public",
        "LOCAL_FOCUS": false,
        "PACKAGE_JSON_PATH": "./"
      }
    }
  },
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.2.0",
    "focus-components": "0.8.5",
    "focus-core": "0.12.3",
    "i18next-client": "^1.11.0",
    "immutable": "^3.7.2",
    "jquery": "3.0.0-alpha1",
    "lodash": "^3.10.1",
    "material-design-lite": "1.0.6",
    "moment": "^2.10.3",
    "native-promise-only": "^0.8.0-a",
    "numeral": "^1.5.3",
    "source-map-loader": "^0.1.5"
  },
  "devDependencies": {
    "animate.css": "^3.4.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-eslint": "4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-runtime": "^1.0.7",
    "babel-runtime": "^5.8.24",
    "better-npm-run": "0.0.5",
    "body-parser": "^1.14.1",
    "css-loader": "^0.21.0",
    "eslint": "1.5.1",
    "eslint-config-focus": "0.3.0",
    "eslint-plugin-filenames": "0.1.1",
    "eslint-plugin-react": "3.5.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "font-awesome": "^4.5.0",
    "json-loader": "^0.5.3",
    "material-design-icons-iconfont": "^2.0.4",
    "migration-tool": "^0.1.0",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.11.0",
    "webpack-focus": "^0.8.6"
  }
}
